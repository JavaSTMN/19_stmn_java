package exercice2;

import java.util.Calendar;
import java.util.Date;
import java.util.GregorianCalendar;

public class EventItem {
	
	Calendar time;
	Object source;
	String type;
	
/*	public EventItem(String type, Object src) {
		this.source = src;
		this.type = type;
	}*/
	
	public String toString() {
		//time = new Date();
		Date date = new Date();
		time = new GregorianCalendar();
		time.setTime(date);
		
		
		String test = time.get(Calendar.YEAR)+ "/" + time.get(Calendar.MONTH);
		System.out.println(test);
		return type;
		
	}

}
